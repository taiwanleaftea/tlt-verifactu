{
    "name": "taiwanleaftea/tlt-verifactu",
    "description": "Laravel library for EU VAT validation and VERIFACTU support",
    "keywords": [
        "laravel", "verifactu", "AEAT", "VIES"
    ],
    "homepage": "https://github.com/taiwanleaftea/tlt-verifactu",
    "type": "library",
    "authors": [
        {
            "name": "MCQRus.com",
            "email": "info@mcqrus.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-soap": "*",
        "chillerlan/php-qrcode": "dev-main",
        "illuminate/contracts": "^11.0||^12.0",
        "robrichards/xmlseclibs": "^3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Taiwanleaftea\\TltVerifactu\\": "src/"
        },
        "files": []
    },
    "autoload-dev": {
        "psr-4": {
            "Taiwanleaftea\\TltVerifactu\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Taiwanleaftea\\TltVerifactu\\Providers\\TltVerifactuServiceProvider"
            ],
            "aliases": {}
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    }
}
